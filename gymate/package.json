{
  "name": "gymate",
  "version": "1.0.0",
  "description": "Gym social networking app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "docker:build": "docker build -t gymate-app .",
    "docker:run": "docker run -p 5000:5000 gymate-app",
    "docker:compose": "docker-compose up --build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["gym", "social", "networking", "firebase"],
  "author": "Emma Claw",
  "license": "MIT"
}